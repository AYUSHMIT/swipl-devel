.NOCHECK

ARCH=mswin-3.1
VERSION=4.8.0, June 1994

MAKE=wmake /h
CC=wcc386
CPlusPlus=wpp386
CWFLAGS=
COFLAGS=/d1
CIFLAGS=-I.. -I..\..\..
CFLAGS=$(CWFLAGS) $(COFLAGS) $(CIFLAGS) /bt=windows
LD=wlink system win386
WST=msw

LN=copy

MKPROTO=mkproto -p
CLPROTO=grep -vw NewClass
ETAGS=etags

.c.obj:
	$(CC) /zq $(CFLAGS) $*.c

.cxx.obj
	$(CPlusPlus) /zq $(CFLAGS) $*.cxx
